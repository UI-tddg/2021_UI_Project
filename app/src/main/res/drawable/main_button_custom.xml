<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--평상시-->
    <item android:state_pressed="false">
        <shape>
            <!--배경-->
            <solid android:color="#FFF1EF" />
            <!--테두리-->
            <stroke
                android:width="0dp"
                android:color="#FFF1EF" />
            <corners
                android:radius="30dp"/>
        </shape>
    </item>
</selector>
